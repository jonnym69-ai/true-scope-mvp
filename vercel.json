{
  "version": 2,
  "builds": [
    {
      "src": "true-scope-mvp/**/*",
      "use": "@vercel/static"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/true-scope-mvp/$1"
    }
  ]
}
